import java.util.*;
public class Solution {

	public static void main(String[] args) {
        
        Scanner s= new Scanner(System.in);
        int num = s.nextInt();
	
        int i = 1;
        while(i<=num){
            int j=1;
                while(j<=i){
                    char ch=(char)('A' + i -1);
                    System.out.print(ch);
                    j=j+1;
                }
            System.out.println();
            i=i+1;
        }
	}
}
